---
strongswan:
  #lookup:

  config:
    strictcrlpolicy: 'yes'
    uniqueids: 'no'

  conn:
    conn_a:
      left: 10.10.1.1
      leftsubnet: 10.10.1.0/32
      right: 10.20.1.1
      rightsubnet: 10.20.1.0/32
      ike: 3des-sha1-modp1024
      esp: 3des-sha1
      authby: secret
      auto: add

    conn_b:
      left: 10.10.2.1
      leftsubnet: 10.10.2.0/32
      right: 10.20.2.1
      rightsubnet: 10.20.1.0/32
      ike: 3des-sha1-modp1024
      esp: 3des-sha1
      authby: secret
      auto: add

